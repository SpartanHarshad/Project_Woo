<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/sky_Darkblue"
    tools:context=".About_Me">

    <TextView
        android:id="@+id/tvgotochooseinterest"
        style="@style/txt_TopBACK"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvgotoCity"
        style="@style/txt_topNEXT"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvAboutMe"
        style="@style/txt_AoutMe"
        tools:ignore="MissingConstraints"/>

    <EditText
        android:id="@+id/etvAboutme"
        style="@style/Etxt_Aoutme"
        android:textColorHint="@color/white"
        tools:ignore="MissingConstraints"
        android:backgroundTint="@color/white"
        android:gravity="start"/>

    <TextView
        android:id="@+id/tvKeepclean"
        android:paddingHorizontal="@dimen/dp_20"
        style="@style/txt_Keepclean"
        tools:ignore="MissingConstraints"/>


</androidx.constraintlayout.widget.ConstraintLayout>