    <style name="btn_signin">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
        <item name="layout_constraintBottom_toTopOf">@id/tvsignfacebook</item>
        <item name="android:background">@drawable/sign_in_button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:drawableLeft">@drawable/ic_cell_phone</item>
        <item name="android:drawablePadding">@dimen/dp_20</item>
        <item name="android:text">SIGN-IN WITH PHONE NUMBER</item>
    </style>

